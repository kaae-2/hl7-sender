{
  "name": "hl7-sender",
  "version": "0.1.0",
  "description": "HL7 Sending utility for testing a downstream HL7 processing system.",
  "main": "index.js",
  "scripts": {
    "build": "elm-make src/Main.elm --output elm.js --yes",
    "watch": "chokidar \"**/*.elm\" --initial -c \"npm run build\"",
    "debug": "electron main.js --debug",
    "start": "electron main.js",
    "dev": "npm run watch & npm run debug"
  },
  "keywords": [
    "hl7",
    "hl7-sender",
    "downstream",
    "tcp",
    "port",
    "test",
    "testing",
    "tester"
  ],
  "author": "Matthew King <making3dev@gmail.com> (https://making3.github.io/)",
  "license": "MIT",
  "devDependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "chokidar-cli": "^1.2.0",
    "electron": "^1.8.7",
    "electron-context-menu": "^0.9.1",
    "elm": "^0.18.0"
  }
}
