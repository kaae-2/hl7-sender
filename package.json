{
  "name": "hl7-sender",
  "version": "0.1.0",
  "description": "HL7 Sending utility for testing a downstream HL7 processing system.",
  "main": "index.js",
  "scripts": {
    "elm": "elm make src/Main.elm --output elm.js || true",
    "elm:watch": "./node_modules/.bin/chokidar '**/*.elm' -c 'npm run elm'",
    "debug": "electron main.js --debug",
    "start": "electron main.js",
    "watch": "npm run debug & npm run elm:watch"
  },
  "keywords": [
    "hl7",
    "hl7-sender",
    "downstream",
    "tcp",
    "port",
    "test",
    "testing",
    "tester"
  ],
  "author": "Matthew King <making3dev@gmail.com> (https://making3.github.io/)",
  "license": "MIT",
  "devDependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "chokidar-cli": "^1.2.0",
    "electron": "^1.7.10",
    "electron-context-menu": "^0.9.1"
  }
}
